		public void Main()
		{
            int leidos = (int)Dts.Variables["User::Leidos"].Value;
            int tipo1 = (int)Dts.Variables["User::SCD1"].Value;
            int tipo2 = (int)Dts.Variables["User::SCD2"].Value;
            int errores = (int)Dts.Variables["User::Errores"].Value;

            double porcentajeCambios = 0.0;
            double porcentajeErrores = 0.0;
            if (leidos > 0)
            {
                porcentajeCambios = ((double)(tipo1 + tipo2) / (double)leidos) * 100.0;
                porcentajeErrores = ((double)errores / leidos) * 100.0;
            }
            Dts.Variables["User::PorcentajeCambios"].Value = porcentajeCambios;
            Dts.Variables["User::PorcentajeErrores"].Value = porcentajeErrores;
            Dts.TaskResult = (int)ScriptResults.Success;
